# 服务配置
server:
  port: 9093

# eureka注册中心配置
eureka:
  client:
    serviceUrl:
      defaultZone: http://localhost:8181/eureka/

# spring配置
spring:
  application:
    name: showcase-consumer
  main:
    allow-bean-definition-overriding: true
  datasource:
    url: jdbc:mysql://127.0.0.1:3306/showcase
    username: root
    password: root
    type: com.alibaba.druid.pool.DruidDataSource